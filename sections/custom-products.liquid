{% render 'card-product' %}



{% schema %}
  {
    "name": "Custom Product Section",
    "tag": "section",
    "class": "section",
    "settings": [
      {
        "type": "text",
        "id": "main_title",
        "default": "Multicolumn",
        "label": "Main Title"
      }, {
        "type": "text",
        "id": "main_content",
        "default": "Multicolumn",
        "label": "Main Content"
      }, {
        "type": "text",
        "id": "main_button_text",
        "default": "Multicolumn",
        "label": "Button Text"
      }, {
        "type": "url",
        "id": "main_button_link",
        "label": "Button Url"
      }
    ],
    "blocks": [
      {
        "type": "column",
        "name": "t:sections.multicolumn.blocks.column.name",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "t:sections.multicolumn.blocks.column.settings.image.label"
          }, {
            "type": "text",
            "id": "cat_title",
            "default": "Multicolumn",
            "label": "Title"
          }, {
            "type": "url",
            "id": "button_link",
            "label": "t:sections.multicolumn.settings.button_link.label"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Custom Product Section",
        "blocks": [
          {
            "type": "column"
          }, {
            "type": "column"
          }, {
            "type": "column"
          }
        ]
      }
    ]
  }
{% endschema %}